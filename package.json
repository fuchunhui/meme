{
  "name": "meme",
  "version": "3.0.0",
  "description": "a meme robot",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "debug": "cross-env NODE_ENV=test node --inspect-brk server.js",
    "test": "node test/setup.js && cross-env NODE_ENV=test mocha",
    "test:db": "node test/setup.js && cross-env NODE_ENV=test mocha test/db.*.test.js",
    "test:api": "node test/setup.js && cross-env NODE_ENV=test mocha test/api.test.js",
    "test:shell": "sh .draft/test-shell.sh",
    "test:message": "node .draft/test-msg.js",
    "t": "node .draft/test-main.js",
    "view:db": "sh .draft/view-db.sh",
    "view:sync": "sh .draft/view-sync.sh",
    "view:public": "sh .draft/view-public.sh"
  },
  "type": "module",
  "dependencies": {
    "axios": "^1.13.2",
    "canvas": "^3.2.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^5.2.1",
    "gif-encoder-2": "^1.0.5",
    "gif-frames": "^1.0.1",
    "sql.js": "^1.13.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "mocha": "^11.7.5",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuchunhui/meme.git"
  },
  "keywords": [
    "robot",
    "ai",
    "image",
    "text",
    "meme",
    "funny",
    "gif",
    "emoji",
    "meme-generator",
    "meme-generator-robot"
  ],
  "author": "fuchunhui",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fuchunhui/meme/issues"
  },
  "homepage": "https://github.com/fuchunhui/meme#readme",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "canvas"
    ],
    "onlyBuiltDependencies": [
      "canvas"
    ]
  }
}
